{
	"name": "app",
	"version": "1.0",

	"engine": {"architecture": "starlark", "version": "1"},
	"files": [ "app.star" ],
	"database": {"file": "app.db", "create": "database_create"},

	"icons": [
		{"path": "app", "name": "App Publisher", "icon": "app.png"}
	],

	"paths": {
		"app": {
			"actions": {
				"": {"function": "action_list"},
				"create": {"function": "action_create"},
				"new": {"function": "action_new"},
				":app": {"function": "action_view"},
				":app/:name": {"function": "action_view"}
			}
		}
	},

	"services": {
		"app": {
			"events": {
				"get": {"function": "event_get"},
				"put": {"function": "event_put"},
				"versions": {"function": "event_versions"}
			}
		}
	}
}
